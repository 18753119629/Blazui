    <None Include="wwwroot\element\fonts\icomoon.eot" />
    <None Include="wwwroot\element\fonts\icomoon.svg" />
    <None Include="wwwroot\element\fonts\icomoon.ttf" />
    <None Include="wwwroot\element\fonts\icomoon.woff" />
    <None Include="wwwroot\element\index.css" />
